        "locales": [
            "bn-IN",
            "zh-CN",
            "zh-TW",
            "da-DK",
            "nl-NL",
            "en-AU",
            "en-CA",
            "en-GB",
            "fi-FI",
            "fr-FR",
            "fr-CA",
            "de-DE",
            "hi-IN",
            "it-IT",
            "ja-JP",
            "ko-KR",
            "nb-NO",
            "pl-PL",
            "pt-BR",
            "ru-RU",
            "es-ES",
            "es-419",
            "sv-SE",
            "tr-TR"
        ],
        "localeMap": {
            "bn-IN": "bn",
            "zh-CN": "zh-CN",
            "zh-TW": "zh-TW",
            "da-DK": "da",
            "nl-NL": "nl",
            "en-AU": "en-AU",
            "en-CA": "en-CA",
            "en-GB": "en-GB",
            "fi-FI": "fi",
            "fr-FR": "fr",
